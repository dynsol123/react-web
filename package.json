{
  "name": "dynsol-web",
  "version": "1.0.0",
  "description": "react app",
  "keywords": [
    "react,heroku, hooks"
  ],
  "author": "Krzysztof Czaplejewicz",
  "license": "MIT",
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@atlaskit/atlassian-navigation": "^1.0.0",
    "@atlaskit/avatar": "^20.3.1",
    "@atlaskit/button": "^15.1.7",
    "@atlaskit/css-reset": "^6.0.5",
    "@atlaskit/datetime-picker": "^10.3.0",
    "@atlaskit/form": "^8.2.2",
    "@atlaskit/icon": "^21.6.1",
    "@atlaskit/logo": "^13.4.2",
    "@atlaskit/menu": "^1.0.3",
    "@atlaskit/motion": "^1.0.1",
    "@atlaskit/page": "^12.0.6",
    "@atlaskit/page-layout": "^1.0.3",
    "@atlaskit/popup": "^1.0.7",
    "@atlaskit/side-navigation": "^1.0.1",
    "@atlaskit/spinner": "^15.0.9",
    "@atlaskit/textarea": "^4.1.1",
    "@atlaskit/textfield": "^5.0.3",
    "@atlaskit/theme": "^11.3.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/browser-image-compression": "^1.0.9",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/validator": "^13.6.3",
    "browser-image-compression": "^1.0.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.4",
    "react-phone-input-2": "^2.14.0",
    "react-query": "^3.18.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.0",
    "typescript": "^4.1.2",
    "validator": "^13.6.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-typescript",
      "airbnb/hooks",
      "plugin:@typescript-eslint/recommended",
      "plugin:jest/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "react",
      "@typescript-eslint",
      "jest"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module",
      "project": "./tsconfig.json"
    },
    "rules": {
      "linebreak-style": "off",
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ],
      "import/prefer-default-export": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "react/jsx-props-no-spreading": "off",
      "react/require-default-props": "off",
      "react/react-in-jsx-scope": "off",
      "react/no-unused-prop-types": "off",
      "no-console": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-dropzone": "^5.1.0",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "prettier": "^2.3.2"
  }
}
